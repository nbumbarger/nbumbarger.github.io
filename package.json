{
  "name": "nbumbarger.github.io",
  "version": "1.0.0",
  "description": "Nick's personal site",
  "repository": {
    "type": "git",
    "url": "https://github.com/nbumbarger/nbumbarger.github.io.git"
  },
  "author": {
    "name": "Nick Bumbarger",
    "url": "http://www.nickbumbarger.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nbumbarger/nbumbarger.github.io/issues"
  },
  "homepage": "http://www.nickbumbarger.com",
  "scripts": {
    "postinstall": "[ -f app/assets/scripts/config/local.js ] || echo \"'use strict';\n\nexport default {\n  environment: 'development'\n};\" > app/assets/scripts/config/local.js",
    "serve": "gulp collecticons && PAGES_ENV=development gulp serve",
    "build": "gulp collecticons && PAGES_ENV=production NODE_ENV=production gulp",
    "lint": "eslint app/assets/scripts/ --ext .js",
    "test": "PAGES_ENV=testing nyc ava test -c 4",
    "report": "nyc report"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "ignore": "test",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ],
      "require-globify",
      "envify"
    ]
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.13",
    "browserify": "^14.4.0",
    "collecticons-processor": "^3.1.0",
    "del": "^3.0.0",
    "envify": "^4.1.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-cache": "^0.4.6",
    "gulp-csso": "^3.0.0",
    "gulp-exit": "0.0.2",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.3.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rev": "^8.0.0",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^3.1.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "gulp-useref": "^3.1.2",
    "gulp-util": "^3.0.8",
    "mapbox-gl-js-mock": "^0.28.0",
    "node-bourbon": "^4.2.8",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.5.3",
    "nyc": "^11.0.3",
    "proxyquire": "^1.8.0",
    "react-test-renderer": "^15.6.1",
    "require-globify": "^1.4.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "@turf/bearing": "^4.5.2",
    "classnames": "^2.2.5",
    "lodash.defaultsdeep": "^4.6.0",
    "mapbox-gl": "^0.39.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-motion": "^0.5.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6"
  }
}
